{
  "name": "frontend-dings",
  "version": "1.0.0",
  "description": "Demoapp that uses TokenX",
  "main": "index.js",
  "scripts": {
    "dev-client": "npx parcel src/client/index.html",
    "test-client": "npx jest --testPathPattern=src/client",
    "build-client": "npx rimraf dist/client && npx parcel build --no-source-maps -d dist/client src/client/index.html",
    "dev-server": "NODE_ENV=development node src/server/index.js",
    "test-server": "npx jest --testPathPattern=src/server",
    "build-server": "npx rimraf dist/server && npm run test-server && npx parcel build --no-source-maps --target node -d dist/server src/server/index.js"
  },
  "author": "https://nais.io/",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "node-fetch": "^2.6.0",
    "openid-client": "^3.15.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2"
  }
}
